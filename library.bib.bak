@article{Colantonio1998,
abstract = {Power-amplifier class-F operation is investigated and revised, evidencing the fundamental importance of the harmonic-generating mechanism and the limitations im- posed by the device input and output nonlinearities on the ideal class-F behavior. Closed- form expressions are derived for the major design quantities, together with the optimum fundamental and third-harmonic loading of the active device. A design procedure, making use of the derived expressions, is presented and the deviations from the ideal behavior are discussed. Sample designs, making use of a full nonlinear device model and commercial analysis software, are carried out to demonstrate the effectiveness of the analysis and the design procedure. It is shown, both analytically and using commercial nonlinear simula- tions, that blind application of commonly used, idealized class-F harmonic terminations can cause unexpected detrimental results.},
annote = {o   Class F definition: terminating the device [amplifier] output with open circuit terminations at odd harmonic frequencies of the fundamental component and short circuiting it at even harmonics 
o   With large signal drive, the inputs are no longer purely sinusoidal and have harmonic content 
{\S}  Generation of harmonics is due to clipping from the forward conduction of the gate to pinch off of conduction 
{\S}  Harmonic termination – tuned load if every harmonic component is loaded by an ideal short circuit termination 
{\textperiodcentered}        Values of harmonic terminations for odd components [2,17,18] 
o   Snider [2] – optimally loaded and overdriven PA to derive the correct termination at haromincs 
o   Output device capacitance will eventually short out high order harmonics 
o   Alpha is the conduction angle of the amplifier 
o   optimum operation of class-F amplifiers [7-9] and indicate some useful guidelines for optimum design [10-12] 
o   Fifth harmonic peaking in [5,6] 
o   3rd harmonic must be out of phase with fundamental in order for ideal square wave nature. If in phase, will result in non ideal waveform. Bias must be between B and AB (deep AB) 
{\S}  Class B bias cannot be used for practical class F 
{\S}  Open termination for odd harmonics is only a mathematical result and doesn't explain mechanism for generating the odd harmonics 
{\S}  Closed form equations and all that jazz!},
author = {Colantonio, Paolo and Giannini, Franco and Leuzzi, Giorgio and Limiti, Ernesto and Elettronica, Ingegneria and Vergata, Tor},
file = {:X$\backslash$:/Google Drive/Thesis/PA/Papers/class f/Colantonio - On the Class-F Power Amplifier Design.pdf:pdf},
journal = {International Journal of RF and Microwave Computer-Aided Engineering},
number = {2},
pages = {129--149},
title = {{On the Class-F Power Amplifier Design}},
volume = {9},
year = {1998}
}
