@article{Colantonio2001,
abstract = {Multiharmonic manipulation is presented as the most effective solution to improve power amplifier (PA) efficiency performances. Remarkable improvements in output power, power gain and power-added efficiency (PAE) are demonstrated, properly manipulating the input and output second and third harmonics, as compared to more classical design approaches. Experimental results at 5GHz confirm the feasibility, the validity and effectiveness of the proposed approach, increasing the maximum measured power-added efficiency from 39{\%} to 61{\%}   {\textcopyright} 2001 John Wiley {\&} Sons, Inc. Int J RF and Microwave CAE 11: 366-384, 2001},
