@article{Raab2002,
abstract = {The generation of RF/microwave power is required not only in$\backslash$nwireless communications, but also in applications such as jamming,$\backslash$nimaging, RF heating, and miniature dc/dc converters. Each application$\backslash$nhas its own unique requirements for frequency, bandwidth, load, power,$\backslash$nefficiency, linearity, and cost. RF power is generated by a wide variety$\backslash$nof techniques, implementations, and active devices. Power amplifiers are$\backslash$nincorporated into transmitters in a similarly wide variety of$\backslash$narchitectures, including linear, Kalm, envelope tracking, outphasing,$\backslash$nand Doherty. Linearity can be improved through techniques such as$\backslash$nfeedback, feedforward, and predistortion},
